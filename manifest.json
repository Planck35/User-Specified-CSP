{
  "name": "Part3 Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "pop.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions": [
    "webRequestBlocking",
    "tabs",
    "storage",
    "cookies",
    "webRequest",
    "http://*/*",
    "https://*/*",
    "file:///*/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}